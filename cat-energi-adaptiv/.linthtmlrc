{
  "extends": "linthtml-config-htmlacademy",
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "composes"
        ]
      }
    ],
    "unit-whitelist": ["em", "rem", "s", "px", "%", "fr", "dppx", "dpi", "deg", "vw"],
    "codeguide/indentation": "tab",
    "codeguide/number-leading-zero": null
  }
}
